{
  "name": "help",
  "rules": {
    "help_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "line"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "\n"
              },
              {
                "type": "SYMBOL",
                "name": "line"
              }
            ]
          }
        }
      ]
    },
    "header": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "tag"
        }
      ]
    },
    "line": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "_atom"
      }
    },
    "_atom": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "word"
        },
        {
          "type": "SYMBOL",
          "name": "tag"
        }
      ]
    },
    "word": {
      "type": "PATTERN",
      "value": "[^*'|\\n]\\S*[^*'|\\n]"
    },
    "tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "*"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "word"
          }
        },
        {
          "type": "STRING",
          "value": "*"
        }
      ]
    },
    "option": {
      "type": "PATTERN",
      "value": "'[a-z]+'"
    },
    "hotlink": {
      "type": "PATTERN",
      "value": "|[a-z-]+|"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[\\t ]"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

