{
  "name": "help",
  "rules": {
    "help_file": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "line"
      }
    },
    "header": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "tag"
        }
      ]
    },
    "line": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "REPEAT1",
            "content": {
              "type": "SYMBOL",
              "name": "_atom"
            }
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "\n"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "_atom": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "word"
        },
        {
          "type": "SYMBOL",
          "name": "tag"
        }
      ]
    },
    "word": {
      "type": "PATTERN",
      "value": "[^*|'\\n \\t]+"
    },
    "tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "*"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "word"
          }
        },
        {
          "type": "STRING",
          "value": "*"
        }
      ]
    },
    "option": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "'"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "word"
          }
        },
        {
          "type": "STRING",
          "value": "'"
        }
      ]
    },
    "hotlink": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "|"
        },
        {
          "type": "FIELD",
          "name": "destination",
          "content": {
            "type": "SYMBOL",
            "name": "word"
          }
        },
        {
          "type": "STRING",
          "value": "|"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[\\t ]"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

